{
    "name": "dominicarendse/everlytic",
    "description": "Provides a simple and elegant wrapper for the Everlytic Bulk Mail API to enable use within laravel framework",
    "keywords":["everlytic","email","bulk"],
    "authors": [
        {
            "name": "Dominic Arendse",
            "email": "me@dominicarendse.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "^4"
    },
    "autoload": {
        "classmap": [
            "src/lib",
            "src/Facades",
            "src/Everlytic.php"
        ],
        "psr-0": {
            "Chencha\\Everlytic\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
